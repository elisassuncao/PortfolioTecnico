    private void cadastrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        Cachorro c = new Cachorro();
        c.setNome(camponome.getText());
        c.setIdade(Integer.parseInt(campoidade.getText()));
        c.setSexo(camposexo.getText());
        c.setRaca(camporaca.getText());
        c.setDono(campodono.getText());
        c.setPorte(campoporte.getText());
        c.setPelagem(campopelagem.getText());
        c.setTelefone(campotelefone.getText());
        c.setPeso(Float.parseFloat(campotelefone.getText()));
        c.setFocinheira(campofocinheira.getValueIsAdjusting());
        if(campofocinheira.getValue() == 1){
            c.setFocinheira(true);
        }else{
              c.setFocinheira(false);
        }
        dogs.add(c);
        JOptionPane.showMessageDialog(this, "O " + camponome.getText() + " foi " 
                + "cadastrado com sucesso ");
    }                                         

